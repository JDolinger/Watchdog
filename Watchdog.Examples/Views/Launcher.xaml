<Window x:Class="Watchdog.Examples.Views.Launcher"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Launcher" >
    <Grid Width="500" Height="200">
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        
        <Button Margin="20, 70" Content="Launch" Click="LaunchClick"/>
        <Button Grid.Column="2" Margin="20, 70" Content="GC" Click="GcClick"/>
    </Grid>
</Window>
